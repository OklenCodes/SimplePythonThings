#Understanding functions

def hello_world():
    print("Hello World!")

hello_world()

def sum(num1, num2):
    print(num1 + num2)

sum(2, 3)
sum(1, 3)
sum(200, 4)

def sum(num1, num2):
    return num1 + num2
total = sum(2, 3)
print(total)

def sum(num1, num2):
    if (type(num1) is not int or type(num2) is not int ):
        return
    return num1 + num2
total = sum("a", 3)
print(total)
